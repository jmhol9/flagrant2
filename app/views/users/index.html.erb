<table class="table">
    <tr class="table__row--title">
        <td class="table__title">Users <span class="body body--italic">(<%= @users.length %> registered)</span></td>
        <td class="table__item table__item--right table__title"># Entries</td>
    </tr>
    <% @users.each do |user| %> 
    <tr class="table__row">
        <td class="table__item">
            <span class="table__team-name">
                <%= user.team_name %> 
            </span>
            <span class="table__user-name">
                <%= user.name %> 
            </span>
            <span class="table__city">
                <%= user.city%>  
            </span>
        </td>
        <td class="table__item table__item--right">
            <%= user.entries.length %>
        </td>
    </tr>
    <% end %>
</table>